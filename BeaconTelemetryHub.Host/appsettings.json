{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "BeaconReceiverSettings": {
    "AdapterName": null,
    "EstimoteServiceUUID": "fe9a",
    "EstimoteTelemetryPacketTypeId": 2,
    "ScanDuration": "00:00:10",
    "ScanInterval": "00:00:01"
  }
}

