{
  "ConnectionStrings": {
    "DefaultConnection": "server=127.0.0.1;port=3306;database=beacons_data;user=root;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "BeaconTelemetryHubSettings": {
    "AdapterName": null,
    "EstimoteServiceUUID": "fe9a",
    "EstimoteTelemetryPacketTypeId": 2,
    "ScanDuration": "00:00:10",
    "ScanInterval": "00:00:01"
  }
}

